{
    "name": "hanaboso/rest-bundle",
    "description": "Hanaboso - rest-bundle",
    "license": "Apache-2.0",
    "version": "1.3.0",
    "autoload": {
        "psr-4": {
            "Hanaboso\\RestBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hanaboso\\RestBundleTests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "hanaboso/utils": "^1.4",
        "symfony/dependency-injection": "^7.0",
        "symfony/serializer": "^7.0"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.5",
        "symfony/yaml": "^7.0"
    },
    "conflict": {
        "friendsofsymfony/rest-bundle": "*",
        "nelmio/cors-bundle": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
